{
  "manifest_version": 3,
  "name": "CodeCycle",
  "version": "0.9.9.61",
  "author": "Albert Dolkun",
  "description": "CodeCycle helps master LeetCode problems through repetition based on Ebbinghaus's Forgetting Curve. Built upon PMCA by Haolin Zhong.",
  "homepage_url": "https://github.com/ADolkun/CodeCycle",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "128": "assets/logo.png",
    "48": "assets/logo.png",
    "16": "assets/logo.png"
  },
  "action": {
    "default_icon": "assets/logo.png",
    "default_popup": "popup.html"
  },
  "background": {},
  "permissions": [
    "https://leetcode.com/*",
    "https://leetcode.cn/*",
    "unlimitedStorage",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["dist/leetcode.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://leetcode.cn/*"],
      "js": ["dist/leetcodecn.js"],
      "run_at": "document_idle"
    }
  ]
}
